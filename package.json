{
  "name": "mywp-starter",
  "displayName": "MyWP Starter",
  "description": "Masmons Wordpress Starter Kit untuk menggenerate directory dan file untuk membuat Wordpress Theme.",
  "author": {
    "name": "Budi Haryono",
    "email": "mail.budiharyono@gmail.com"
  },
  "icon": "assets/icon.png",
  "publisher": "Budi Haryono",
  "version": "7.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/itcomindo/mywp-starter.git",
    "directory": "./mywp-starter",
    "branch": "main",
    "tag": "Wordpress Theme Starter Kit"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:extension.masmonsWPStarter"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^4.3.2",
    "vscode": "^1.1.37",
    "tslint": "^5.14.0",
    "@types/node": "^14.0.0",
    "@types/mocha": "^9.0.0"
  },
  "dependencies": {},
  "contributes": {
    "commands": [
      {
        "command": "extension.masmonsWPStarter",
        "title": "Masmons WP Starter"
      }
    ]
  }
}
